/* pages/demo02/demo02.wxss */
/* 输入框样式定义 */
.inputbox{
  width: 715rpx;
  height: 232rpx;

  border:7rpx solid black;
  border-radius: 45rpx;
  margin-left: 13rpx;
  margin-top: 8rpx;
}
.title1{
  display: flex;
}
.subtitle1{ 
  width: 160rpx;
  height: 45rpx;
  text-align: center;
  line-height: 50rpx;

  border:3rpx solid black;
  border-radius: 10rpx;
  margin-left: 112rpx;
  margin-top: 20rpx;
  font-size: 30rpx;
}
.subtitle2{
  width: 160rpx;
  height: 45rpx;
  text-align: center;
  line-height: 50rpx;

  border:3rpx solid black;
  border-radius: 10rpx;
  margin-left: 175rpx;
  margin-top: 20rpx;
  font-size: 30rpx;
}
.content1{
  display: flex;
}
.subcontent{
  width: 300rpx;
  height: 60rpx;

  border:3rpx solid#66CCFF;
  border-radius: 20rpx;
  margin-top: 10rpx;
  margin-left: 36rpx;
  text-align: center;
  line-height: 60rpx;
}
.place-holder{
  font-size: 30rpx;
  text-align: center;
}
.operate{
  display: flex;
}
.suboperate1{
  
  width: 120rpx;
  height: 60rpx;

  border:3rpx solid#66CCFF;
  border-radius: 20rpx;
  margin-top: 10rpx;
  margin-left: 128rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 35rpx;
}
.suboperate2{
  
  width: 120rpx;
  height: 60rpx;

  border:3rpx solid#66CCFF;
  border-radius: 20rpx;
  margin-top: 10rpx;
  margin-left: 218rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 35rpx;
}
/* 补库清单样式定义 */
.generalList{
  width: 715rpx;
  height: 900rpx;

  border:7rpx solid black;
  border-radius: 45rpx;
  margin-left: 13rpx;
  margin-top: 20rpx;
}
.title2{
  width: 100%;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
}
.t-head {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 70rpx;
  align-items: center;
 }
 .th {
  width: 40%;
  justify-content: center;

  color:black;
  display: flex;
  height: 70rpx;
  align-items: center;
 }
 .t-body{
  display: flex;
  flex-direction: row;
}
.tb1{
  flex-direction: column;
  margin-left: 30rpx;
  width: 210rpx;
  height: 100%;
}
.tb2{
  flex-direction: column;
  margin-left: 89rpx;
  width: 180rpx;
  height: 100%;
}
.btnDel{
  width: 125rpx;
  height: 45rpx;
  line-height: 45rpx;
  font-size: 30rpx;
  margin-left: 35rpx;
}
.btn-area{
  display: flex;
}
.btn_sub{
  margin-top: 20rpx;
  padding: 0;
  width: 150rpx;
  height: 62rpx;
  text-align: center;
  line-height: 50rpx;
